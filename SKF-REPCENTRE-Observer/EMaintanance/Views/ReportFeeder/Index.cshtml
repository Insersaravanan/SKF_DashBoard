@{
    ViewData["Title"] = "Report Feeder";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/lib/angular/ui-grid/ui-grid.min.css" rel="stylesheet" />
<div ng-controller="skfCtrl" class="joblist content-wrapper">

    <div class="container-fluid trending rds" id="rds">
        <div class="wizard-inner">
            <div class="connecting-line"></div>
            <ul class="tabs">
                <li class="list-tab" ng-class="{'active':equipmentListActive}">
                    <a ng-click="next('stage0')" Title="JobList">
                        <span class="round-tab job-round">
                            Job(s)
                        </span>
                        <i class="fa fa-play rf-play" aria-hidden="true"></i>
                    </a>
                </li>
                <li class="list-tab" ng-class="{'active':equipmentActive}" ng-disabled="!isEqActive">
                    <a ng-click="next('stage1')" Title="Equipment List">
                        <span class="round-tab equipment-round">
                            Equipment(s)
                        </span>
                        <i class="fa fa-play rf-play" aria-hidden="true"></i>
                    </a>
                </li>
                <li class="list-tab" ng-class="{'active':DriveActive}" ng-disabled="!isunitActive">
                    <a Title="Drive Unit" ng-click="next('stage2') ">
                        <span class="round-tab asset-round">
                            Asset(s)
                        </span>
                    </a>
                </li>
            </ul>
        </div>
        <div class="animate-switch-container" ng-switch on="stage">

            <div class="trending animate-switch" ng-switch-default>
                <div class="row heading-wrapper">
                    <div class="col-md-3 heading">
                        <h4 class="page-header text-primary">Search Jobs</h4>
                    </div>
                    <div class="col-md-9 menu-button">
                        <span ng-repeat="val in legendDDL" class="legend-text">
                            <span class="legend-color" ng-style="{'background-color': val.LValue }">
                            </span>
                            &nbsp; {{val.Descriptions}} &nbsp;
                        </span>
                    </div>
                </div>
                <div class="row search-form">
                    <div class="col-md-12 col-sm-10">
                        <div class="col-md-2 col-sm-6 input-field">
                            <label class="control-label">From Date*:</label>
                            <p class="input-group date-picker date-wrapper">
                                <input type="text" class="form-control date-picker-input" ng-dblclick="d_ScheduleDate=true" placeholder="dd/mm/yyyy" uib-datepicker-popup="dd/MM/yyyy"
                                       ng-model="S_SelectedJob.StartDate" ng-disabled="readOnlyPage" name="ScheduleDate" ng-init="d_ScheduleDate=false" is-open="d_ScheduleDate"
                                       datepicker-options="dateOptions" required ng-required="true" close-text="Close" alt-input-formats="['M!/d!/yyyy']" />
                                <span class="input-group-btn date-picker-button">
                                    <button type="button" class="btn btn-default" ng-disabled="readOnlyPage" ng-click="d_ScheduleDate=true"><i class="fa fa-calendar"></i></button>
                                </span>
                            </p>
                        </div>
                        <div class="col-md-2 col-sm-6 input-field">
                            <label class="control-label">To Date:</label>
                            <p class="input-group date-picker date-wrapper">
                                <input type="text" class="form-control date-picker-input" ng-dblclick="d_FDate=true" placeholder="dd/mm/yyyy" uib-datepicker-popup="dd/MM/yyyy"
                                       ng-model="S_SelectedJob.EstEndDate" name="FromDate" ng-init="d_FromDate=false" is-open="d_FromDate"
                                       datepicker-options="dateOptions" required ng-required="true" close-text="Close" alt-input-formats="['M!/d!/yyyy']" />
                                <span class="input-group-btn date-picker-button">
                                    <button type="button" class="btn btn-default" ng-click="d_FromDate=true"><i class="fa fa-calendar"></i></button>
                                </span>
                            </p>
                        </div>
                        <div class="col-md-2 col-sm-6 input-field">
                            <label class="control-label">Job Number</label>
                            <div>
                                <input class="form-control" name="AssetName" ng-model="S_SelectedJob.JobNumber" />
                                <span class="text-danger"></span>
                            </div>
                        </div>
                        <div class="col-md-1 col-sm-6 buttons create filter-button">
                            <button ng-click="loadData()" class="btn btn-info"><i class="fa fa-binoculars icon-filter"></i>Go</button>
                        </div>
                        <div class="col-md-5 rf-tab">
                            <table class="tooltip-sec small-first-col">
                                <thead>
                                    <tr>
                                        <th>Data collector</th>
                                        <th>Analyst</th>
                                        <th>Reviewer</th>
                                        <th>Report sent</th>
                                    </tr>
                                </thead>
                                <tbody class="body-half-screen">
                                    <tr ng-repeat="val in JobStatusColourDDL" ng-if="JobStatusColourDDL.length > 0">
                                        <td><span class="legend-color fortab" ng-style="{'background-color': val.DCStatus }"></span>{{val.DataCollector}}</td>
                                        <td><span class="legend-color fortab" ng-style="{'background-color': val.ALStatus }"></span>{{val.Analyst}}</td>
                                        <td><span class="legend-color fortab" ng-style="{'background-color': val.RVStatus }"></span>{{val.Reviewer}}</td>
                                        <td><span ng-show="{{val.ReportSent}} == 1"><i class="fa fa-check tick" aria-hidden="true"></i></span></td>
                                    </tr>
                                    <tr ng-if="JobStatusColourDDL.length <= 0">
                                        <td colspan="4">No Data Avaliable</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                    @*<div class="buttons create filter-button col-md-2  col-sm-2">
                            <button ng-click="loadData()" class="btn btn-info"><i class="fa fa-binoculars icon-filter"></i>Go</button>
                        </div>*@
                </div>
                <div class="table-responsive job-list-grid">
                    <div class="grid data">
                        <div id="uiGrid" ui-grid="gridOpts" class="grid" ui-grid-cellnav ui-grid-edit ui-grid-pinning ui-grid-resize-columns ui-grid-exporter></div>
                    </div>
                </div>
            </div>

            <div class="equipment">
                <form name="userForm" novalidate>
                    <div class="equipment animate-switch" ng-switch-when="stage1">
                        <div class="row heading-wrapper">
                            <div class="col-md-3 heading">
                                <h4 class="page-header text-primary">Equipment List</h4>
                            </div>
                            <div class="col-md-9 menu-button">
                                <span ng-repeat="val in legendDDL" class="legend-text">
                                    <span class="legend-color" ng-style="{'background-color': val.LValue }">
                                    </span>
                                    &nbsp; {{val.Descriptions}} &nbsp;
                                </span>
                            </div>
                        </div>
                        <div class="row create-form">
                            <div class="col-md-12">
                                <div class="col-md-3 col-sm-6 input-field">
                                    <label class="control-label">Job Number</label>
                                    <div>
                                        <input disabled class="form-control" value={{SelectedJob.JobNumber}} name="JobNumber" />
                                        <span class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="col-md-3 col-sm-6 input-field">
                                    <label class="control-label">Job Name</label>
                                    <div>
                                        <input disabled class="form-control" value="{{SelectedJob.JobName}}" name="JobName" />
                                        <span class="text-danger"></span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="table-responsive">
                            <div class="grid data">
                                <div id="uiGrid" ui-grid="gridOpts1" class="grid" ui-grid-cellnav ui-grid-pinning ui-grid-resize-columns ui-grid-exporter></div>
                            </div>
                        </div>
                    </div>
                </form>
            </div>

