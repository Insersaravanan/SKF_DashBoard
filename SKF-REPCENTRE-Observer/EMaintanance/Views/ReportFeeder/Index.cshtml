<script type="text/ng-template" id="skfStatus.html">
    <form name="skfForm" ng-submit="save()" novalidate class="settings_popup pop-header">
        <div class="modal-header">
            <span class="model-breadcrumb">Job Status </span>
            <div class="col-lg-5 col-md-5 col-sm-12 heading">
                <h4 class="page-header text-primary">{{JobNumber}} - {{JobName}}</h4>
            </div>
            <div class="col-lg-7 col-md-7 col-sm-12 create-button  menu-button">
                <span class="Pipe"></span>
                <a ng-click="save()" title="Save"><i class="fa fa-save icon-filter"></i><span>Save</span></a>
                <a ng-click="cancel()" title="Close"><i class="fa fa-close icon-filter"></i><span>Close</span></a>
            </div>
        </div>
        <div class="modal-body">
            <div class="row">
                <div class="col-md-4">
                    <div class="status-label">
                        <label class="control-label"><b>Data Collection Date*:</b></label>
                        <p class="input-group date-picker date-wrapper">
                            <input type="text" class="form-control date-picker-input" ng-dblclick="d_DataCollectionDate=true" placeholder="dd/mm/yyyy" uib-datepicker-popup="dd/MM/yyyy"
                                   ng-change="ChangeColDate()" ng-model="DataCollectionDate" name="DataCollectionDate" ng-init="d_DataCollectionDate=false" is-open="d_DataCollectionDate" ng-disabled="IsEditable==0"
                                   datepicker-options="DColdateoptions" ng-required="true" close-text="Close" alt-input-formats="['M!/d!/yyyy']" />
                            <span class="input-group-btn date-picker-button">
                                <button type="button" class="btn btn-default" ng-click="d_DataCollectionDate=true"><i class="fa fa-calendar"></i></button>
                            </span>
                    </div>
                    <div class="status-label">
                        <label class="control-label">
                            <b>Data Collection Completed:</b>
                            <input type="checkbox" ng-disabled="!isDCDoneAllowed" class="data-collection form-control" ng-model="DataCollectionStatus" name="DataCollectionStatus" ng-true-value="'Y'" ng-false-value="'N'" />
                        </label>
                    </div>

                    <div class="input-field">
                        <label class="control-label"><b>Report Date*:</b></label>
                        <p class="input-group date-picker date-wrapper">
                            <input type="text" class="form-control date-picker-input" ng-dblclick="d_ReportDate=true" placeholder="dd/mm/yyyy" uib-datepicker-popup="dd/MM/yyyy"
                                   ng-model="ReportDate" name="ReportDate" ng-init="d_ReportDate=false" is-open="d_ReportDate" ng-disabled="JobReportEditable==0"
                                   datepicker-options="reportdateoptions" ng-required="true" close-text="Close" alt-input-formats="['M!/d!/yyyy']" />
                            <span class="input-group-btn date-picker-button">
                                <button type="button" class="btn btn-default" ng-click="d_ReportDate=true"><i class="fa fa-calendar"></i></button>
                            </span>
                        </p>
                    </div>
                    <div class="status-label">
                        <label class="control-label">
                            <b>Report Sent to Customer:</b>
                            <input type="checkbox" ng-disabled="!ReportSentAllowed" class="data-collection form-control" ng-model="ReportSentStatus" name="ReportSent" ng-true-value="'Y'" ng-false-value="'N'" />
                        </label>
                    </div>
                    <p class="status-label">
                        <b>Status</b>
                        <select ng-show="!isJcStatus" ng-disabled="jobStatusList.length <= 0 || !Datacollection" ng-hide="jobStatusList.length == 0" ng-model="StatusId" class="form-control search-input" name="StatusId" ng-change="AssignedTo(StatusId)"
