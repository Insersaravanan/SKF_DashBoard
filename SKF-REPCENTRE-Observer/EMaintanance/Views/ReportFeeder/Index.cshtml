                                    <div uib-accordion-group is-open="Accstatus.isSecondOpen" class="Accordion-heading" heading="{{ServiceName}} - Non Drive End and Drive End Analysis Sets">
                                        <table class="drive-set" ng-repeat="val in VADriveData">
                                            <tr class="first-row">
                                                <td class="first-column" colspan="3">
                                                    <h5 class="control-label symptom-title">{{val.SymptomType}} - {{val.Frequency}} Frequency</h5>
                                                </td>
